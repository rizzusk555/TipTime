<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/enterence">

    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.tiptime.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <fragment
        android:id="@+id/enterence"
        android:name="com.example.tiptime.Enterence"
        android:label="fragment_enterence"
        tools:layout="@layout/fragment_enterence" >
        <action
            android:id="@+id/action_enterence_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
</navigation>